<view><scroll-view class="page" scroll-y="{{modalName==null}}"><cu-custom vue-id="7fa66ab1-1" bgColor="{{NavBarColor}}" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">个人信息</view></cu-custom><view class="cu-list menu-avatar margin-top"><view class="cu-item"><block wx:if="{{infoList.avatar}}"><view class="cu-avatar round lg" style="{{'background-image:'+('url('+infoList.avatar+')')+';'}}"></view></block><block wx:else><view class="cu-avatar round lg" style="{{'background-image:'+('url('+avatar+')')+';'}}"></view></block><view class="content"><view class="text-grey text-lg">{{infoList.realname?infoList.realname:infoList.username}}<text class="{{['cuIcon-peoplefill','margin-left-sm',infoList.sex=='2'?'text-pink':'text-blue']}}"></text></view><view class="text-gray text-sm flex"><view class="text-cut">{{''+infoList.post+''}}</view></view></view></view></view><view class="cu-list menu margin-top"><view class="cu-item animation-slide-bottom" style="{{'animation-delay:'+('0.2s')+';'}}"><view class="content"><text class="text-grey">手机</text></view><view class="action"><text class="text-grey">{{infoList.phone?infoList.phone:phone}}</text></view></view><view class="cu-item animation-slide-bottom" style="{{'animation-delay:'+('0.3s')+';'}}"><view class="content"><text class="text-grey">邮箱</text></view><view class="action"><text class="text-grey">{{infoList.email?infoList.email:email}}</text></view></view><view class="cu-item animation-slide-bottom" style="{{'animation-delay:'+('0.4s')+';'}}"><view class="content"><text class="text-grey">部门</text></view><view class="action"><text class="text-grey">{{infoList.departName?infoList.departName:company}}</text></view></view></view></scroll-view></view>